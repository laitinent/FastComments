<Window x:Class="FastComments.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastComments"
        xmlns:p="clr-namespace:FastComments.Properties"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.rw_title}" Height="450" Width="800">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <TextBox x:Name="tbContents" Margin="10,0,0,0" TextWrapping="Wrap" ToolTip="{x:Static p:Resources.rw_title}" IsReadOnly="True"/>
        </ScrollViewer>
    </Grid>
</Window>
