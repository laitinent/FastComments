<Window x:Class="FastComments.HelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastComments"
        mc:Ignorable="d"
        Title="Help" Height="354.038" Width="414.284">
    <Grid>
        <ListView x:Name="listView1" SelectionChanged="listView1_SelectionChanged" >
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Code" DisplayMemberBinding="{Binding Key}">

                    </GridViewColumn>
                    <GridViewColumn Header="Text" DisplayMemberBinding="{Binding Fulltext}"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
